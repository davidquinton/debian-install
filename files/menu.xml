		<menu id="root-menu-77123" label="Files                              &gt;">
			<item label="Copy Xfce notes">
				<action name="Execute">
					<execute>sh -c &quot;rsync -ru $HOME/.local/share/notes $(yad --title='Copy Xfce notes' --file --directory --height=400 --width=500)&quot;</execute>
				</action>
			</item>
			<item label="Config files">
				<action name="Execute">
					<execute>sh ~/scripts/yad-configfiles</execute>
				</action>
			</item>
			<item label="open odt">
				<action name="Execute">
					<execute>sh -c &quot;libreoffice4.4 $(yad --title='Select file' --file --filename=$HOME/ --height=400 --width=500)&quot;</execute>
				</action>
			</item>
			<item label="view pdf                  .">
				<action name="Execute">
					<execute>sh -c &quot;xpdf $(yad --file --title='Select pdf file' --filename=$HOME/Documents/ --height=400 --width=500)&quot;</execute>
				</action>
			</item>
			<item label="Send a file to Trash">
				<action name="Execute">
					<execute>sh -c &quot;mv $(yad --title='Warning! Sending file to the trash!' --file --filename=$HOME/ --height=400 --width=500) $HOME/.local/share/Trash/files/&quot;</execute>
				</action>
			</item>
		</menu>
